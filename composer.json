{
    "name": "agenter-labs/illuminate-gate",
    "description": "Auth checker for Laravel",
    "keywords": [
        "laravel",
        "auth",
        "checker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/cache": "^8.0",
        "agenter-labs/illuminate-token": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "mockery/mockery": "0.9.*",
        "laravel/lumen-framework": "^8.0"
    },
    "autoload": {
    "psr-4": {
            "AgenterLab\\Gate\\": "./src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\Gate\\GateServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
