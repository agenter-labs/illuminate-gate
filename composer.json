{
    "name": "agenter-labs/illuminate-gate",
    "description": "Auth checker for Laravel",
    "keywords": [
        "laravel",
        "auth",
        "checker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/cache": "^9.0",
        "firebase/php-jwt": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "0.9.*",
        "laravel/lumen-framework": "^9.0"
    },
    "autoload": {
    "psr-4": {
            "AgenterLab\\Gate\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\Gate\\GateServiceProvider"
            ]
        }
    }
}
